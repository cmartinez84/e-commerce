{{user.email}}
  {{#each cart.items as |item|}}
  <div class="panel">
      <li>Name: {{item.name}}</li>
      <img src="{{item.image}}" alt="" onerror='this.src="https://heatherchristenaschmidt.files.wordpress.com/2011/09/facebook_no_profile_pic2-jpg.gif"' />
      Description: {{item.description}}
      Price: {{item.price}}
      <button class="btn btn-danger"{{action "removeItem" item}}>X</button>
  </div>
  {{/each}}
<h1>{{cart.total}}</h1>
